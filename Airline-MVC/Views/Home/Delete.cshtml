@model Airline_MVC.Models.AirlineTicket
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete the Ticket?</h1>

<form asp-action="Delete">
    <input type="hidden" asp-for="TicketNum" />
    <input class="btn btn-dark" type="submit" value="Yes" />
    <a class="btn btn-danger" asp-action="Index">No</a>
</form>